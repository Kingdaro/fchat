{
  "defaultSeverity": "warn",
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "ordered-imports": [
      false
    ],
    "object-literal-sort-keys": false,
    "member-access": [
      false
    ],
    "arrow-parens": false,
    "interface-name": [
      false
    ],
    "no-empty": false,
    "no-console": [
      false
    ],
    "max-classes-per-file": [
      false
    ],
    "trailing-comma": [
      false
    ],
    "curly": false
  },
  "rulesDirectory": []
}
